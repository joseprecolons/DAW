
public class ClassePrincipal {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		int comptador;
		
		comptador = 0;
		
		while (comptador < 10) {
			comptador = (comptador + 1);
			System.out.println(comptador);
		}
		
		while (comptador > 1) {
			comptador = (comptador - 1);
			System.out.println(comptador);
		}
		
		System.out.println("El programa ha finalitzat");
	}

}
